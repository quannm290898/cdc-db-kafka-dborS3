{
  "name": "mysql-sink-connector",
  "config": {
    "connector.class": "io.confluent.connect.jdbc.JdbcSinkConnector",
    "tasks.max": "1",
    "topics": "cdc.transaction",
    "connection.url": "jdbc:mysql://<ip_adress>:3306/cdc_database",
    "connection.user": "root",
    "connection.password": "<pass_word>",
    "insert.mode": "insert",
    "auto.create": "true",
    "auto.evolve": "true",
    "delete.enabled": "false",
    "pk.mode": "none",
    "table.name.format": "${topic}",
    "transforms": "unwrap,route",
    "transforms.unwrap.type": "io.debezium.transforms.ExtractNewRecordState",
    "transforms.unwrap.drop.tombstones": "true",
    "transforms.route.type": "org.apache.kafka.connect.transforms.RegexRouter",
    "transforms.route.regex": "cdc\\.(.*)",
    "transforms.route.replacement": "$1",
    "key.converter": "io.apicurio.registry.utils.converter.AvroConverter",
    "key.converter.apicurio.registry.url": "http://<ip_adress>:8085/apis/registry/v2",
    "key.converter.apicurio.registry.auto-register": "true",
    "key.converter.apicurio.registry.find-latest": "true",
    "value.converter": "io.apicurio.registry.utils.converter.AvroConverter",
    "value.converter.apicurio.registry.url": "http://<ip_adress>:8085/apis/registry/v2",
    "value.converter.apicurio.registry.auto-register": "true",
    "value.converter.apicurio.registry.find-latest": "true"
  }
}
